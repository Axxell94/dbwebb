# Uppgiften är att byta plats på två saker i listan. 
# i main.py skall du lägga till stöd för kommandot “inv swap" : 

bag=["sko", "väska", "flodhäst"]

def swap(bag, thing1, thing2):
    """byt plats på saker"""
                 
    temp= bag[thing1]
    bag[thing1] = bag[thing2]
    bag[thing2]= temp
    print(bag)
    return bag



def main():   # jag tog bort alla onödiga saker här 
    stop=False
    while not stop:
        
        choice = input("--> ")
        if choice == "q":
            stop= Ture

        elif "inv swap" in choice:    # det vill säga man skriver exempelvis "inv swap sko väska" för att kalla på funktionen
            choices = choice.split(" ")
            swap(bag,choices[2], choices[3])


if __name__ == "__main__":
    main()
